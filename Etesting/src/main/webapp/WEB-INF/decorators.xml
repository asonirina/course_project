    <decorator name="layoutAdmin" page="layoutAdmin.jsp">
        <pattern>/*</pattern>
    </decorator>

    <decorator name="layoutStudent" page="layoutStudent.jsp">
        <pattern>/e-Testing/UploadProgram.html</pattern>
        <pattern>/e-Testing/SaveProgram.html</pattern>
