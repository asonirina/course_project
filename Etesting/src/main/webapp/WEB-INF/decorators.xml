<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/jsp/decorator">
    <decorator name="layoutAdmin" page="layoutAdmin.jsp">
        <pattern>/e-Testing/admin/*</pattern>
    </decorator>

    <decorator name="layoutStudent" page="layoutStudent.jsp">
        <pattern>/e-Testing/student/*</pattern>
    </decorator>

    <decorator name="layoutLogin" page="layoutLogin.jsp">
        <pattern>/e-Testing/Login.html</pattern>
        <pattern>/e-Testing/Denied.html</pattern>
        <pattern>/e-Testing/error503.html</pattern>
        <pattern>/e-Testing/error404.html</pattern>
        <pattern>/e-Testing/ResetPassword.html</pattern>
    </decorator>
</decorators>